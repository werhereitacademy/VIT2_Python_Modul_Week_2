#Link 1 - List Comprehensions

if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    m = []
    for i in range(0,x+1):
        for k in range(0,y+1):
            for l in range(0,z+1):
                if (i+k+l) != n:
                    m.append([i,k,l])
    print(m)

