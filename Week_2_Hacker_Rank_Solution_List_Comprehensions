if __name__ == '__main__':
x = int(input())
y = int(input())
z = int(input())
n = int(input())

x = 1
y = 1
z = 1
n = 2

m = []
    for i in range(0,x+1):
        for k in range(0,y+1):
            for l in range(0,z+1):
                if (i+k+l) != n:
                    m.append([i,k,l])
    print(m)
